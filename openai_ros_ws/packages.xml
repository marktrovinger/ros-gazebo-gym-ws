<?xml version="1.0"?>
<package format="2">
    <name>openai_ros_ws</name>
    <version>0.0.0</version>
    <description>
        Openai_ros_ws contains all the ros packages needed to use the openai_ros package.
        This was done since not all packages are deployed to the ROS repositry.
    </description>
    <maintainer email="rick.staa@outlook.com">Rick Staa</maintainer>
    <license>MIT</license>
    <url type="website">https://rickstaa.github.io/openai_ros_ws</url>
    <url type="repository">https://github.com/rickstaa/openai_ros_ws</url>
    <url type="bugtracker">https://github.com/rickstaa/openai_ros_ws/issues</url>
    <author email="rick.staa@outlook.com">Rick Staa</author>

    <buildtool_depend>catkin</buildtool_depend>

    <exec_depend>openai_ros</exec_depend>
    <export>
        <metapackage />
    </export>
</package>